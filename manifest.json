{
    "name": "MIDIUserBlocker",
    "version": "1.0",
    "icons": {
      "64": "icon_64.png",
      "128": "icon_128.png"
    },
    "description": "MIDI.cz user blocker",
    "permissions": ["storage"],
    "content_scripts": [ {
        "matches": ["*://*.midi.cz/*"],
        "js": ["jquery-3.4.1.min.js", "blocker.js"]
      } ],
    "manifest_version": 2
}